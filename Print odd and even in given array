#include<stdio.h>
int main(){
	printf("Name: Jeevitha\nReg No:192324020\n");
	int n,i;
	printf("Enter the size of array :");
	scanf("%d",&n);  
	int a[n];
	printf("\nEnter elements of array :");
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	printf("\nOdd numbers in array: ");
	for(i=0;i<n;i++){
		if(a[i]%2!=0)
		printf("%d ",a[i]);
	}
	printf("\nEven numbers in array: ");
	for(i=0;i<n;i++){
		if(a[i]%2==0)
		printf("%d ",a[i]);
	}
	return 0;
}
