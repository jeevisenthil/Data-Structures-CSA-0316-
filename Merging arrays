#include<stdio.h>
int main()
{
	int n,m,p,a[50],b[50],c[50],i;
	printf("Name:Jeevitha\nReg no:192324020\n");
	printf("Enter size of array 1:\n");
	scanf("%d",&n);
	printf("Enter elements of array 1:\n");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	printf("Enter size of array 2:\n");
	scanf("%d",&m);
	printf("Enter elements of array 2:\n");
	for(i=0;i<m;i++)
	scanf("%d",&b[i]);
	for(i=0;i<n;i++)
	{
		c[i]=a[i];
	}
	for(i=0;i<m;i++)
	{
		c[i+n]=b[i];
	}
	printf("Merged array is\n");
	p=m+n;
	for(i=0;i<p;i++)
	{
		printf("%d",c[i]," ");
	}
	return 0;
}
